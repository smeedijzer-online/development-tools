{
  "name": "smeedijzer-online/development-tools",
  "type": "wordpress-muplugin",
  "license": "MIT",
  "description": "Package to be used during active development",
  "homepage": "https://github.com/smeedijzer-online/development-tools",
  "authors": [
    {
      "name": "Smeedijzer",
      "homepage": "https://github.com/smeedijzer-online"
    }
  ],
  "keywords": [
    "wordpress",
    "bedrock",
    "composer"
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org",
      "only": [
        "wpackagist-plugin/*",
        "wpackagist-theme/*"
      ]
    }
  ],
  "require": {
    "php": ">=7.2.5",
    "composer/installers": "^1.10",
    "wpackagist-plugin/query-monitor": "dev-trunk",
    "wpackagist-plugin/duplicate-page": "dev-trunk",
    "wpackagist-plugin/post-type-switcher": "dev-trunk"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
